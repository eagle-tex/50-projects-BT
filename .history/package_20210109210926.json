  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix --ignore-path ./.gitignore",
      "git add -A"
    ],
    "*.+(json|yml|yaml|css|scss|md|vue)": [
      "./node_modules/.bin/prettier --write",
      "git add -A"
    ]
  },
